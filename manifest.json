{
  "manifest_version": 3,
  "name": "我的信息过滤器",
  "version": "1.0",
  "description": "一键收藏 Twitter 和 YouTube 内容到本地知识库",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "添加到过滤器"
  },
  "icons": {
    "128": "icon.png"
  }
}

